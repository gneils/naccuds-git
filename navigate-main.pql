subprocedure VALIDATE_TEMP_DOB
. ifthen ( exists( cdate( temp_dob, 'MM/DD/YYYY' ) ) eq NO )
.   display infobox 'Sorry, The birthday is not in the correct format.'
.   focus item FIND_DOB
.   PROCEED# = NO
. endif
