subprocedure SET_TEMP_VALUES
.  TEMP_CASE$ = trim( gettxt( FIND_CASE ) )
.  TEMP_CASE# = numbr( TEMP_CASE$ )
.  TEMP_DOB =  gettxt( FIND_DOB )
