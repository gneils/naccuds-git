subprocedure SET_VISIT
C write /// 'SET visit' MESSAGE_COUNTER
. VISIT$ = trim( sbst( getitxt( VISITS, getpos( VISITS ) ), 1, 2 ) )
. set item VISITS_LABEL, 'Visit: ' + VISIT$
. VISIT# = numbr( VISIT$ )
