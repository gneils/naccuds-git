. subprocedure GOTO_SCREEN_NACCUDS
C   WHICH SUFFIX SHOULD BE USED FOR THE PQL FORMS NAME
C   FORMS WILL TRY FALLING BACK.  LATEST APPROPRIATE VERSION FIRST THEN OLDER VERSIONS
C   TRANSFORM SELECTED_SCREEN IF NEEDED TO NEW NAME
<comment> write 'debugging screen member: ' SELECTED_SCREEN
.   SCREEN_MEMBER = SELECTED_SCREEN | then build up screen member to point to the right member
.   ifthen ( SELECTED_SCREEN EQ 'A1DEMOG' or SELECTED_SCREEN EQ 'A2INFORM' )
.     ifthen ( exists( PACKET$ ) EQ 0 )
.       display infobox "Packet Type is missing"
.       EXIT SUBPROCEDURE
.     endif
.     if ( PACKET$ EQ 'T' ) | telephone packets get followup packet screens
.     SCREEN_MEMBER = SELECTED_SCREEN + '_' + 'F'
.   elseif ( SCREEN_MEMBER = 'A3FAMHIS' )
.     if ( PACKET$ = 'T' ) | telephone packets get followup packet screens
.     SCREEN_MEMBER = SELECTED_SCREEN + '_' + 'F'
.   elseif ( SCREEN_MEMBER = 'A4MEDS' )
.     ifthen ( ( VISIT_DATE >= V2_DATE ) AND ( VISIT_DATE <V3_DATE ) )
.       SCREEN_MEMBER = 'A4G'
.     endif
.   endif
C   FIND THE RELEVANT VERSION OF THE PQL FORM
.   IFTHEN (VISIT_DATE < V2_DATE )
<COMMENT> write 'Before Version 2' VISIT_DATE
.     if (meminfo (DB_NAME + '.SCREENS.' + SCREEN_MEMBER + ':T', 1 ) eq 1) FORM_SUFFIX = ''
.     if (meminfo (DB_NAME + '.SCREENS.' + SCREEN_MEMBER + '_V1' + ':T', 1 ) eq 1) FORM_SUFFIX = '_V1'
.   ELSEIF (VISIT_DATE < V3_DATE )
<COMMENT> write 'Before Version 3' VISIT_DATE
.     if (meminfo (DB_NAME + '.SCREENS.' + SCREEN_MEMBER + ':T', 1 ) eq 1) FORM_SUFFIX = ''
.     if (meminfo (DB_NAME + '.SCREENS.' + SCREEN_MEMBER + '_V1' + ':T', 1 ) eq 1) FORM_SUFFIX = '_V1'
.     if (meminfo (DB_NAME + '.SCREENS.' + SCREEN_MEMBER + '_V2' + ':T', 1 ) eq 1) FORM_SUFFIX = '_V2'
.   ELSE | latest Version: at time of writing V3
<COMMENT> write 'Must be Version 3' VISIT_DATE
.     if (meminfo (DB_NAME + '.SCREENS.' + SCREEN_MEMBER + ':T', 1 ) eq 1) FORM_SUFFIX = ''
.     if (meminfo (DB_NAME + '.SCREENS.' + SCREEN_MEMBER + '_V1' + ':T', 1 ) eq 1) FORM_SUFFIX = '_V1'
.     if (meminfo (DB_NAME + '.SCREENS.' + SCREEN_MEMBER + '_V2' + ':T', 1 ) eq 1) FORM_SUFFIX = '_V2'
.     if (meminfo (DB_NAME + '.SCREENS.' + SCREEN_MEMBER + '_V3' + ':T', 1 ) eq 1) FORM_SUFFIX = '_V3'
.   ENDIF
.   CHECK_NAME = DB_NAME + '.SCREENS.' + SCREEN_MEMBER + FORM_SUFFIX + ':T'

<COMMENT> write [meminfo( CHECK_NAME, 1)] CHECK_NAME '| ' DB_NAME  '.SCREENS.'  SCREEN_MEMBER  FORM_SUFFIX  ':T'

.   FULL_SCREEN_NAME = DB_NAME + '.SCREENS.' + SCREEN_MEMBER + FORM_SUFFIX
C   CHECK TO SEE IF SCREEN EXISTS AND IF IT IS CORRECTLY COMPILED
.   ifNOTthen (meminfo(FULL_SCREEN_NAME + ':O', 1) = MEM_O)
.     display infobox 'Contact Help Support cannot find ' + fst(FULL_SCREEN_NAME, SELECTED_SCREEN, 'UNK SCREEN')
.     exit subprocedure
.   endif
.   ifNOTthen (meminfo(FULL_SCREEN_NAME+ ':O', 8) EQ 0)  | correctly compiled
.     display infobox 'Contact Help Support COMPILE ' + fst(SCREEN_MEMBER, SELECTED_SCREEN, 'unk SCREEN')
.     exit subprocedure
.   endif
<COMMENT> WRITE 'HERE IS THE PASSED VALUE: ' PACKET$
.   execute subroutine [FULL_SCREEN_NAME] ( CASE$ KEY1$ KEY2$ KEY3$ KEY4$ PACKET$ KEY6$ )
.   ifthen ( SELECTED_SCREEN = 'Z1CHECK' or SELECTED_SCREEN = 'Z1TELE' )
.     execute subprocedure LOAD_VISITS
.     execute subprocedure SET_VISIT
.   endif
. end subprocedure
