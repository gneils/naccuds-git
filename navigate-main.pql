subprocedure SET_SELECTED_SCREEN
. ifthen( getfocus( 0 ) eq ADRC_SCREENS )
.   SELECTED_SCREEN = trim( sbst( gettxt( ADRC_SCREENS ), 1, 9 ) )
. elseif( getfocus( 0 ) eq NACCUDS_SCREENS )
.   SELECTED_SCREEN = trim( sbst( gettxt( NACCUDS_SCREENS ), 1, 9 ) )
. elseif( getfocus( 0 ) eq PICK_MODULE ) | Selecting a module will reset screen selection
.   SELECTED_SCREEN = trim( sbst( gettxt( ADRC_SCREENS ), 1, 9 ) )
. else
.    write 'wrong time to call this subprocedure'
. endif
end subprocedure

