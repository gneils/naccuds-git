. subprocedure LOAD_ADRC_DATES
.  remove all KEY2_VALUE
.  append item KEY2_VALUE, 'New ' + SELECTED_SCREEN
.  ifNOTTHEN ( SELECTED_SCREEN eq 'BBIOP' OR
               'BLOOD' OR
               'BRAIN' OR
               'CONTACT' OR
               'CTMRI' OR
               'PETSPECT' )
.    EXIT SUBPROCEDURE
.  endif
.  database is ADRC
.    case is CASE#
.      ifthen ( SELECTED_SCREEN eq 'BBIOP' )
.         process rec BBIOP
.          append item KEY2_VALUE, datec( BDATE, 'MM/DD/YYYY' )
.        end rec
.      elseif ( SELECTED_SCREEN eq 'BLOOD' )
.        process rec BLOOD
.          append item KEY2_VALUE, datec( BL_FORM, 'MM/DD/YYYY' )
.        end rec
.      elseif ( SELECTED_SCREEN eq 'BRAIN' )
.        process rec BRAIN
.          append item KEY2_VALUE, datec( DATE14, 'MM/DD/YYYY' )
.        end rec
.      elseif ( SELECTED_SCREEN eq 'CONTACT' )
.        process rec CONTACT
