                         $7  with (VISIT#)$
                         34
                         18
                         33
                         50
                         $75 with (VISIT#)$
                         12
.        THIS_REC_NAME = 'REC$'
.        GOT_ONE = NO
.        process rec REC#
.          GOT_ONE = YES
.        end rec
.        execute subprocedure APPEND_FORM_FEEBACK
.      end repeat
.    end case
.  end database is
. ANY_NUM = scrollto( FORM_FEEDBACK, REMEMBER_SCROLL)
. end subprocedure LOAD_FORM_FEEDBACK
